<!-- <!DOCTYPE html> -->
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hostar all listings</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://hostar.onrender.com/css/style.css">
    <link rel="stylesheet" href="https://hostar.onrender.com/css/style2.css">
    
</head>
     <style>
        .navbar form{
           display: none !important;
        }
     </style>
<body>
    <%-include("../includes/navbar.ejs");%> 
    <%-include("../includes/sidebar.ejs")%>
    <h3 class="heading">Lates Releases</h3>
            <div class="cards-container cardsContainer" id="cardsContainer">
                <!-- Cards -->
                <%for(let d of data){%>
                    <div class="card smallcard" onclick="submitCardData('<%= d._id %>')">
                        <img src="<%=d.poster.url%>" alt="Card Image" class="card-img">
                        <div class="card-info">
                            <h3><%=d.title%></h3>
                            <p><%=d.description%></p>
                        </div>
                    </div>
                    <%}%> 
            </div>
    <script src="/js/script.js"></script>
</body>

</html>
